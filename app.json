{
	"expo": {
		"name": "quick-maths",
		"slug": "quick-maths",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/math-icon-background.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/math-splashscreen.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"adaptiveIcon": {
				"foregroundImage": "./assets/math-icon.png",
				"backgroundColor": "#cccccc"
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/math-icon.png",
				"backgroundColor": "#cccccc"
			}
		},
		"web": {
			"favicon": "./assets/math-icon.png",
			"backgroundColor": "#cccccc"
		}
	}
}
